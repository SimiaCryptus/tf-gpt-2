<project name="tf-gpt-2">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.8</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>false</sideBarEnabled>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
            <gitHub>
                <projectId>SimiaCryptus/tf-gpt-2</projectId>
                <ribbonOrientation>left</ribbonOrientation>
                <ribbonColor>black</ribbonColor>
            </gitHub>
            <profile>release</profile>
        </fluidoSkin>
    </custom>
    <body>
        <menu ref="reports"/>
        <menu name="Simia Cryptus" position="right">
            <item name="Blog" href="http://simiacryptus.com"/>
            <item name="GitHub" href="https://github.com/SimiaCryptus/"/>
            <item name="Modules" href="http://code.simiacrypt.us/release/2.0.0/all-projects/index.html"/>
            <item name="Tests" href="http://code.simiacrypt.us/tests/com/simiacryptus/mindseye/index/BasicTestReport.html"/>
            <item name="Maven" href="https://mvnrepository.com/artifact/com.simiacryptus"/>
        </menu>

        <footer></footer>

    </body>
</project>
